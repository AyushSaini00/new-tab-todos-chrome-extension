{
  "manifest_version": 3,
  "name": "New Tab Todos Chrome Extension",
  "description": "Dead-simple Todos for new tab on chrome",
  "version": "1.0.0",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
